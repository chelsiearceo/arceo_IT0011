
rows = 5

print("A:")
for i in range(1, rows + 1):
    for j in range(rows - i):
        print(" ", end="")
    for k in range(1, i + 1):
        print(k, end="")
    print()  

print("\nB:")

# Initialize the list of numbers
numbers = [1, 3, 5, 6, 7]

# Initialize index
index = 0

while index < len(numbers):
    num = numbers[index]
    count = 0
    while count < num:
        print(str(num) * num)
        break  # To ensure the number prints only once per line
    index += 1
