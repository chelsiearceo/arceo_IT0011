rows = 5

print("A:")
for i in range(1, rows + 1):
    for j in range(rows - i):
        print(" ", end="")
    for k in range(1, i + 1):
        print(k, end="")
    print()  

print("")
print("B:")

numbers = [1, 3, 5, 6, 7]  
index = 0 

while index < len(numbers):  
    num = numbers[index]  
    count = 0  
    while count < num:  
        print(num, end="")  
        count += 1  
    print()  
    index += 1  
